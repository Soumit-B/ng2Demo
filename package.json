{
  "name": "iCabsApp",
  "version": "1.0.4",
  "description": "iCabs on Angular2",
  "repository": {
    "type": "bitBucket",
    "url": "https://bitbuket.com/"
  },
  "author": {
    "name": "iCabs",
    "email": ""
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=6.4"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=DEV webpack --display-chunks",
    "build:prod": "npm run clean && npm run clean:aot && cross-env NODE_ENV=PROD webpack  --display-chunks",
    "build:prod:aot": "npm run clean && cross-env NODE_ENV=PROD npm run webpack -- --config webpack.config.aot.js --display-chunks",
    "build:prod:live": "npm run clean && npm run clean:aot && cross-env NODE_ENV=PRODLIVE webpack  --display-chunks",
    "build:prod:live:aot": "npm run clean && cross-env NODE_ENV=PRODLIVE npm run webpack -- --config webpack.config.aot.js --display-chunks",
    "build:qa": "npm run clean && npm run clean:aot && cross-env NODE_ENV=QA webpack --display-chunks",
    "build:qa:aot": "npm run clean && cross-env NODE_ENV=QA npm run webpack -- --config webpack.config.aot.js --display-chunks",
    "build:bkp": "cross-env NODE_ENV=BACKUP webpack --display-chunks",
    "compile:aot": "./node_modules/.bin/ngc -p ./tsconfig.aot.compile.json",
    "rollup": "./node_modules/.bin/rollup -c rollup-config.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "del-cli target",
    "clean:aot": "del-cli aot",
    "heroku-postbuild": "npm run build",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js src server",
    "lint:ts": "tslint -c tslint.json -o tslint.log 'src/**/*.ts'",
    "lint:ts:console": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:generate": "tslint --init",
    "prebuild": "npm run clean",
    "pretest": "npm run lint -s",
    "server": "cross-env NODE_ENV=DEV nodemon -w 'server/**/*.*' server/main.js",
    "devserver": "cross-env NODE_ENV=DEV nodemon -w 'dev_Server/**/*.*' dev_Server/server.js",
    "server:dev": "cross-env NODE_ENV=DEV webpack-dev-server --progress --compress",
    "server:bkp": "cross-env NODE_ENV=BACKUP webpack-dev-server --progress",
    "server:devonqa": "cross-env NODE_ENV=DEVONQA webpack-dev-server --progress",
    "server:devonsg": "cross-env NODE_ENV=DEVONSG webpack-dev-server --progress",
    "start": "del-cli report && npm run server:dev",
    "start:prod": "npm run server",
    "start:bkp": "del-cli report && npm run server:bkp",
    "start:devonqa": "del-cli report && npm run server:devonqa",
    "start:devonsg": "del-cli report && npm run server:devonsg",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "version": "npm run changelog && git add CHANGELOG.md",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
    "converter": "node dev_plugins/runConverter",
    "genformcontrols": "node dev_plugins/genFormCtrl"
  },
  "dependencies": {
    "compression": "~1.6.2",
    "express": "~4.14.0",
    "helmet": "~3.1.0",
    "ng2-table": "^1.3.2",
    "prompt": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "winston": "~2.3.0"
  },
  "devDependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/compiler-cli": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/platform-server": "2.2.3",
    "@angular/router": "3.2.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "@ngtools/webpack": "^1.1.9",
    "@nsalaun/ng2-logger": "^1.0.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.41",
    "@types/systemjs": "^0.19.32",
    "angular-2-local-storage": "0.0.23",
    "angular-cli": "^1.0.0-beta.22-1",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "~6.5.3",
    "awesome-typescript-loader": "^3.0.0-beta.10",
    "body-parser": "^1.15.2",
    "cldr-data": "^31.0.2",
    "clipboard": "^1.5.16",
    "codelyzer": "2.0.0-beta.3",
    "compression-webpack-plugin": "^0.4.0",
    "concurrently": "^3.1.0",
    "conventional-changelog-cli": "~1.2.0",
    "copy-webpack-plugin": "~4.0.1",
    "core-js": "~2.4.1",
    "cross-env": "~3.1.3",
    "css-loader": "~0.26.0",
    "del-cli": "~0.2.0",
    "enhanced-resolve": "^3.0.0",
    "es6-shim": "^0.35.2",
    "eslint": "~3.11.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "globalize": "^1.3.0",
    "globalize-webpack-plugin": "^1.1.0",
    "html-replace-webpack-plugin": "^2.2.4",
    "html-webpack-plugin": "~2.24.1",
    "iana-tz-data": "^2017.1.0",
    "immutable": "~3.8.1",
    "jasmine-core": "~2.5.2",
    "json-loader": "~0.5.4",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha-reporter": "~2.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "~1.8.0",
    "method-override": "^2.3.7",
    "morgan": "^1.7.0",
    "ng2-bootstrap": "1.1.16",
    "ng2-select": "^1.2.0",
    "ng2-slim-loading-bar": "^2.0.4",
    "ng2-translate": "^5.0.0",
    "ng2-webstorage": "^1.4.1",
    "node-sass": "~3.13.0",
    "nodemon": "~1.11.0",
    "npm-run-all": "~3.1.0",
    "path": "^0.12.7",
    "postcss-loader": "~1.1.1",
    "raw-loader": "~0.5.1",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "^5.0.0-rc.4",
    "sass-loader": "~4.0.2",
    "source-map-explorer": "^1.3.3",
    "strip-loader": "^0.1.2",
    "style-loader": "~0.13.1",
    "ts-helpers": "~1.1.1",
    "ts-loader": "~1.2.2",
    "tslint": "4.5.1",
    "tslint-loader": "^3.3.0",
    "typescript": "~2.0.3",
    "typings": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-md5-hash": "~0.0.5",
    "webpack-version-file-plugin": "^0.2.2",
    "zone.js": "~0.7.2",
    "moment": "~2.15.1"
  }
}